class Solution {
    public int findTheWinner(int n, int k) {
        
        Queue<Integer> q = new LinkedList<>();
        int size = n;
        for(int i = 0;i<n;i++){
            q.offer(i);
        }
        while(q.size() != 1){
            for(int i = 0;i<k-1;i++){
            int x = q.poll();
            q.offer(x);
            }
            q.poll();
        }
        return q.poll()+ 1;
    }
}
